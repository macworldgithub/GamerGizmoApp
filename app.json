{
  "expo": {
    "name": "GamerGizmo",
    "slug": "gamer-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gamergizmoapp",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to find nearby gamers.",
        "NSLocationAlwaysUsageDescription": "We use your location to provide real-time location features."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gamergizmoapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cacf93cb-eb23-4dbe-b952-8fc0a9cc61ab"
      },
      "googleClientId": "1067596237443-61c50tr1djq6pii5oil7m3he9c2btfj4.apps.googleusercontent.com",
      "googleClientIdAndroid": "663923789268-kpbnv7pi2mph8khmcvq420c18945mje0.apps.googleusercontent.com",
      "googleClientIdIos": "663923789268-8p0ekjpcn7n0r4n60alkoimvc321evdo.apps.googleusercontent.com"
    },
    "owner": "jawwadkareem"
  }
}
